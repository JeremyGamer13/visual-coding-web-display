{
  "name": "visual-coding-web-display",
  "version": "0.0.0",
  "description": "Visual Coding Web Display or VCWD is a rewrite of Blockly by Google using Canvas APIs under the MIT license.",
  "source": "src/index.js",
  "main": "dist/main.js",
  "type": "module",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "scripts": {
    "build": "parcel build src/index.js --dist-dir dist --no-source-maps",
    "dev": "parcel watch 'src/*'",
    "test": "cross-env NODE_OPTIONS='--experimental-vm-modules' jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JeremyGamer13/visual-coding-web-display.git"
  },
  "engines": {
    "node": ">= 12"
  },
  "keywords": [
    "blockly",
    "interface",
    "code"
  ],
  "author": "JeremyGamer13",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JeremyGamer13/visual-coding-web-display/issues"
  },
  "homepage": "https://github.com/JeremyGamer13/visual-coding-web-display#readme",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "parcel": "^2.13.0"
  }
}
